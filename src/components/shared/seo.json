{
  "collectionName": "components_shared_seos",
  "info": {
    "displayName": "SEO"
  },
  "options": {},
  "attributes": {
    "metaTitle": {
      "type": "string",
      "required": true,
      "maxLength": 60
    },
    "metaDescription": {
      "type": "text",
      "minLength": 50,
      "maxLength": 150,
      "required": true
    },
    "metaImage": {
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ],
      "type": "media",
      "multiple": false,
      "required": true
    },
    "keywords": {
      "type": "text",
      "regex": "[^,]+",
      "required": false
    },
    "metaRobots": {
      "type": "text",
      "regex": "[^,]+"
    },
    "metaViewport": {
      "type": "text"
    },
    "canonicalURL": {
      "type": "text"
    },
    "metaSocial": {
      "type": "text"
    }
  }
}
